  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blancoleopardo/gtaw-chatlog-magician/issues"
  },
  "homepage": "https://github.com/blancoleopardo/gtaw-chatlog-magician#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@vitest/ui": "^4.0.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "http-server": "^14.1.1",
    "prettier": "^3.3.3",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
