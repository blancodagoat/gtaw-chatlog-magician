{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not edge <= 128",
    "not chrome <= 112",
    "not ios_saf <= 26",
    "not safari <= 26",
    "not samsung <= 28",
    "not and_ff <= 143",
    "not firefox <= 143"
  ]
}